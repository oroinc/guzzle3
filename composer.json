{
    "name": "oro/guzzle",
    "type": "library",
    "description": "This is a fork of an older verions of PHP HTTP client - https://github.com/guzzle/guzzle3. This library is deprecated in favor of https://packagist.org/packages/guzzlehttp/guzzle",
    "keywords": ["framework", "http", "rest", "web service", "curl", "client", "HTTP client"],
    "homepage": "http://guzzlephp.org/",
    "license": "MIT",

    "authors": [
        {
            "name": "Michael Dowling",
            "email": "mtdowling@gmail.com",
            "homepage": "https://github.com/mtdowling"
        },
        {
            "name": "Guzzle Community",
            "homepage": "https://github.com/guzzle/guzzle/contributors"
        }
    ],

    "replace": {
        "guzzle/batch": "v3.7.4",
        "guzzle/cache": "v3.7.4",
        "guzzle/common": "v3.7.4",
        "guzzle/http": "v3.7.4",
        "guzzle/inflection": "v3.7.4",
        "guzzle/iterator": "v3.7.4",
        "guzzle/log": "v3.7.4",
        "guzzle/parser": "v3.7.4",
        "guzzle/plugin": "v3.7.4",
        "guzzle/plugin-async": "v3.7.4",
        "guzzle/plugin-backoff": "v3.7.4",
        "guzzle/plugin-cache": "v3.7.4",
        "guzzle/plugin-cookie": "v3.7.4",
        "guzzle/plugin-curlauth": "v3.7.4",
        "guzzle/plugin-error-response": "v3.7.4",
        "guzzle/plugin-history": "v3.7.4",
        "guzzle/plugin-log": "v3.7.4",
        "guzzle/plugin-md5": "v3.7.4",
        "guzzle/plugin-mock": "v3.7.4",
        "guzzle/plugin-oauth": "v3.7.4",
        "guzzle/service": "v3.7.4",
        "guzzle/stream": "v3.7.4",
        "guzzle/guzzle": "v3.7.4"
    },

    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "symfony/event-dispatcher": ">=2.1"
    },

    "autoload": {
        "psr-0": {
            "Guzzle\\Tests": "tests/",
            "Guzzle": "src/"
        }
    },

    "require-dev": {
        "doctrine/cache": "*",
        "symfony/class-loader": "*",
        "monolog/monolog": "1.*",
        "psr/log": "1.0.*",
        "zendframework/zend-cache": "2.0.*",
        "zendframework/zend-log": "2.0.*",
        "phpunit/phpunit": "3.7.*"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "3.7-dev"
        }
    }
}
